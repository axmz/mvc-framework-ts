n      .then((res: AxiosResponse) => this.trigger(\"save\"))\n      .catch(err => this.trigger(\"error\"));\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"d9bf7a8fddafeafe6693d46e2081bfdf","cacheData":{"env":{}}}